{
  "name": "lyrics-analyser",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "nodemon server.js",
    "heroku-postbuild": "cd client && npm build && npm start"
  },
  "engines": {
    "node": ">=10.0.0",
  },
  "dependencies": {
    "azlyrics": "^2.0.0",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.2",
    "natural": "^0.6.3",
    "node-fetch": "^2.3.0",
    "querystring": "^0.2.0",
    "simple-get-lyrics": "0.0.4",
    "stemr": "^1.0.0",
    "stopword": "^0.2.1",
    "text-cleaner": "^0.3.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
